{
  "name": "haunted-refactorium",
  "version": "1.0.0",
  "description": "Analyze legacy codebases and reveal cursed code with spooky visualizations",
  "main": "dist/backend/index.js",
  "type": "module",
  "scripts": {
    "dev:backend": "tsx watch src/backend/index.ts",
    "dev:frontend": "vite",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "vite build",
    "build": "npm run build:backend && npm run build:frontend && npm run docs:build",
    "start": "node --import tsx src/backend/index.ts",
    "test": "jest",
    "test:properties": "jest --testMatch='**/*.property.spec.ts'",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "setup": "node scripts/verify-setup.js",
    "verify": "node scripts/verify-setup.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "refactoring",
    "code-analysis",
    "legacy-code",
    "halloween"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.22.3",
    "simple-git": "^3.30.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "fast-check": "^3.17.1",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vitepress": "^2.0.0-alpha.15"
  }
}
